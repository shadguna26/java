public class s{
		public static void main(String[] args) {
				int arr[]  =new int[]{10,20, 30,40};
						int b[] = new int[4];
								for(int i=0;i<arr.length;i++){
					   					b[i]=arr[i];
					   					}
					   					b[2]=99;
					   					for(int i:arr){
					   						System.out.println(i+"");
					   					}
					   					System.out.println();
					   					for(int i: b) {
					   						System.out.println(i+"");
					   					}
					   				}
					   			}
					   	
